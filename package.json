{
  "name": "champion.rdap.client",
  "version": "0.0.1",
  "license": "mrCooper",
  "scripts": {
    "build": "webpack --progress --colors",
    "ci": "npm run e2e && npm run test",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "client": "concurrently \"npm run docker-client\" \"npm run delayed-open\"",
    "delayed-open": "sleep 5 && open http://localhost:2368",
    "docker-client": "webpack-dev-server --host=0.0.0.0 --hot --inline  --progress --port 2368 & wait",
    "docker-open-terminal": "docker exec -i ${PWD##*/} /bin/bash",
    "docker-server": "concurrently \"npm run server\" \"npm run docker-client\"",
    "docker-start": "docker-compose --p ${PWD##*/} up",
    "docker-stop": "docker-compose --p ${PWD##*/} stop",
    "docs": "typedoc --options typedoc.json src/app/app.ts",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "lint": "tsconfig-lint",
    "new-component": "yo ng2-webpack:component",
    "new-directive": "yo ng2-webpack:directive",
    "new-interface": "yo ng2-webpack:interface",
    "new-pipe": "yo ng2-webpack:pipe",
    "new-service": "yo ng2-webpack:service",
    "postinstall": "npm run webdriver-update && typings install",
    "predocker-server": "npm install --quiet",
    "predocker-start": "docker pull cmelion/generator-ng2-webpack:latest",
    "server": "json-server --watch json-server/db.json",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "watch": "webpack --watch --progress --profile",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "^0.2.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.2",
    "@ngrx/store": "^1.5.0",
    "es6-shim": "^0.35.1",
    "es7-reflect-metadata": "1.6.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "lodash": "^4.12.0",
    "phantomjs-polyfill": "0.0.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-notification-system": "^0.2.6",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.6",
    "zone.js": "0.6.15"
  },
  "devDependencies": {
    "@angularclass/hmr": "^1.0.1",
    "@angularclass/hmr-loader": "^1.0.0",
    "@types/core-js": "^0.9.0",
    "@types/jasmine": "^2.2.29",
    "@types/node": "^6.0.38",
    "@types/protractor": "^1.5.16",
    "@types/selenium-webdriver": "^2.44.26",
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.3.2",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "es6-promise-loader": "^1.0.1",
    "exports-loader": "0.6.3",
    "expose-loader": "^0.7.1",
